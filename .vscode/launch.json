{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Ejecutar app.main",
      "type": "debugpy",
      "request": "launch",
      "module": "app.main",
      "console": "integratedTerminal",
      "justMyCode": true,
      "cwd": "${workspaceFolder}/APPODS",
      "python": "${command:python.interpreterPath}",
      "redirectOutput": true
    },
    {
      "name": "Ejecutar main.py directo",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/APPODS/app/main.py",
      "console": "integratedTerminal",
      "justMyCode": true,
      "python": "${command:python.interpreterPath}",
      "redirectOutput": true
    }
  ]
}

